{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-typescript", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["./tsconfig.json"]
  },
  "plugins": ["import"],
  "rules": {
    "max-len": ["warn", 80, { "ignorePattern": "^import .*" }]
  }
}
